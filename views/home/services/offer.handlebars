<!-- Start main-content -->
<div class="main-content">

    <!-- Section: inner-header -->
    <section class="inner-header divider parallax layer-overlay " data-bg-img="images/bg/bg8.webp">
        <div class="container pt-60 pb-60">
            <!-- Section Content -->
            <div class="section-content">
                <div class="row">
                    <div class="col-md-12 text-center">
                        <h1 class="text-white hide">{{service.s_title}}</h1>
                        <h1 class="text-white">{{service.s_title}}</h1>
                        <ol class="breadcrumb text-center mt-10">
                            <li><a href="/" class="text-white">Home</a></li>
                            <li><a href="/services" class="text-white">Services</a></li>
                            <li class="text-white">{{service.s_header}}</li>
                        </ol>
                    </div>
                </div>
            </div>
        </div>
    </section>


    <!-- Section: Blog -->
    <section>

        <div class="container mt-30 mb-30 pt-30 pb-30">
            <div class="row">
                <div class="col-md-9">
                    <div class="blog-posts single-post">
                        <article class="post clearfix mb-0">
                            <div class="entry-header">
                                <div class="post-thumb thumb"> <img src="/uploads/{{service.s_Image}}" alt=""
                                        class="img-responsive img-fullwidth"> </div>
                            </div>
                            <div class="entry-content">
                                <div class="entry-meta media no-bg no-border mt-15 pb-20">
                                    <div class="media-body pl-15" id="{{service.s_id}}">
                                        <input type="hidden" name="{{service.s_slug}}" value="{{service.s_slug}}">
                                        <h1 class="text-title">{{service.s_title}}</h1>
                                        <p>{{ stripTags service.s_content}}</p>
                                    </div>
                                </div>
                            </div>
                        </article>
                    </div>

                    <div class="mt-10 mb-10 pt-10 pb-10">
                        <a href="/services"><i class="fas fa-angle-double-left"></i> Back to Services
                            List</a>
                    </div>
                </div>
                <!--end col-md-9-->

                <div class="col-md-3">
                    <div class="sidebar sidebar-left mt-sm-30">
                        <div class="widget">
                            <h4 class="widget-title">Other Services</h4>
                            <div class="categories">
                                <ul class="list list-border angle-double-right">
                                    {{#each services}}
                                    {{s_title}}
                                    {{/each}}
                                </ul>
                            </div>
                        </div>


                    </div>
                </div>






            </div>
        </div>
    </section>

</div>
<!-- end main-content -->

<script src="https://kit.fontawesome.com/1ff5057583.js"></script>